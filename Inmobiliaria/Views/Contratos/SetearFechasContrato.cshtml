@model Inmobiliaria.Models.Contrato

@{
    ViewData["Title"] = "SetearFechasContrato";
}
@if (TempData["Error"] != null)
{
    @Html.Raw("<strong style='color: red;'> " + TempData["Error"] + "</strong>")
}
<h1>Crear Contrato</h1>

<h4>Ingrese fechas</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create" method="get">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <div class="form-group">
                <label asp-for="FechaDeInicio" class="control-label"></label>
                <input asp-for="FechaDeInicio" value="@TempData["FechaDeInicio"]" class="form-control" />
                <span asp-validation-for="FechaDeInicio" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FechaDeFinalizacion" class="control-label"></label>
                <input asp-for="FechaDeFinalizacion" value="@TempData["FechaDeFinalizacion"]" class="form-control" />
                <span asp-validation-for="FechaDeFinalizacion" class="text-danger"></span>
            </div>
                

            <div class="form-group">
                <input type="submit" value="Siguiente" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a href="@TempData["returnUrl"]">Volver</a>
</div>

