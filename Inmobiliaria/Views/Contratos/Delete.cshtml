@model Inmobiliaria.Models.Contrato

@{
    ViewData["Title"] = "Eliminar";
}
@Html.Raw("<strong style='color: red;'>" + TempData["Error"] + "</strong>")
<h1>Eliminar</h1>

<h4>¿ Esta seguro que quiere eliminar este contrato ?</h4>
<div>
    <h4>Contrato</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdContrato)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdContrato)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Detalle)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Detalle)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Monto)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Monto)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FechaDeInicio)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaDeInicio)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FechaDeFinalizacion)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaDeFinalizacion)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdInquilino)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdInquilino)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.IdInmueble)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdInmueble)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a href="@TempData["returnUrl"]">Volver</a>
    </form>
</div>
